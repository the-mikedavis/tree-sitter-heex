      <%# ^ module %>
      <%#                  ^ attribute %>
      <%#                      ^ string %>
      <%#                            ^ attribute %>
      <%#                                 ^ string %>
      <%#                                        ^ attribute %>
      <%#                                           ^ keyword %>
      <%#                                                    ^ keyword %>
      <%#                                                      ^ punctuation.bracket %>
      <.form />
